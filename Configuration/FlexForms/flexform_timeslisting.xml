<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Assalat Config</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<switchableControllerActions>
						<TCEforms>
							<label>LLL:EXT:assalat/Resources/Private/Language/locallang.xlf:tx_assalat.settings.display</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:assalat/Resources/Private/Language/locallang.xlf:tx_assalat.settings.month</numIndex>
										<numIndex index="1">Salatday->listMonth</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:assalat/Resources/Private/Language/locallang.xlf:tx_assalat.settings.week</numIndex>
										<numIndex index="1">Salatday->list</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:assalat/Resources/Private/Language/locallang.xlf:tx_assalat.settings.day</numIndex>
										<numIndex index="1">Salatday->show</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>

					<settings.update>
						<TCEforms>
							<label>Download new countries and cities on save</label>
							<onChange>reload</onChange>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.update>

					<settings.country>
						<TCEforms>
							<label>Country</label>
							<config>
								<type>select</type>
								<itemsProcFunc>Alat\Assalat\Controller\CountryController->flexformAction</itemsProcFunc>
							</config>
						</TCEforms>
					</settings.country>
					<settings.state>
						<TCEforms>
							<label>State</label>
							<config>
								<type>select</type>
								<itemsProcFunc>Alat\Assalat\Controller\StateController->flexformAction</itemsProcFunc>
							</config>
						</TCEforms>
					</settings.state>
					<settings.city>
						<TCEforms>
							<label>City</label>
							<config>
								<type>select</type>
								<itemsProcFunc>Alat\Assalat\Controller\CityController->flexformAction</itemsProcFunc>
							</config>
						</TCEforms>
					</settings.city>
				</el>
			</ROOT>
		</sDEF>

